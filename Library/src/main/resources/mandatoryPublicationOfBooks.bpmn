    <bpmn:sequenceFlow id="Flow_10pijzb" sourceRef="Gateway_1m87gpi" targetRef="Activity_0l7unilsdv">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision == 'denied'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_052hexh" sourceRef="Gateway_1m87gpi" targetRef="mandatoryBookEvaluationForm">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision == 'vote_again'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="Event_1c7lwsa">
